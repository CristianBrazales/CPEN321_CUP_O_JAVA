{
  "name": "rental_tinder",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "firebase": "^5.0.3",
    "react": "16.6.0-alpha.8af6728",
    "react-native": "^0.57.5",
    "react-native-modal-datetime-picker": "^6.0.0",
    "react-native-router-flux": "^4.0.5",
    "validate.js": "^0.12.0"
  },
  "devDependencies": {
    "babel-jest": "23.6.0",
    "detox": "^9.1.2",
    "jest": "^23.6.0",
    "metro-react-native-babel-preset": "0.48.1",
    "mocha": "^5.2.0",
    "react-test-renderer": "16.6.0-alpha.8af6728"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/rental_tinder.app",
        "build": "xcodebuild -project ios/rental_tinder.xcodeproj -scheme rental_tinder -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 7"
      }
    },
    "test-runner": "mocha"
  }
}